{
  "name": "beatstar",
  "version": "7.7.2",
  "description": "Beatstar JS",
  "main": "./client/index.js",
  "scripts": {
    "git": "git add . && git commit -m 'npm auto commit' && git push && npm version patch",
    "minor": "git add . && git commit -m 'npm next minor version' && git push && npm version minor",
    "release": "git add . && git commit -m 'npm release' && git push && npm version major",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "parcel watch ./client/js/main.js -d ./client --target=electron --no-hmr",
    "build": "parcel build ./client/js/main.js -d ./client --target=electron",
    "start": "electron ./client",
    "run": "npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "http://oriolgomez.com:9900/ogomez92/beatjs"
  },
  "author": "Oriol Gomez",
  "license": "GPL",
  "dependencies": {
    "52-deck": "^1.1.0",
    "agk-ui": "0.0.2",
    "audiobuffer-to-wav": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "base-64": "^0.1.0",
    "copy-to-clipboard": "^3.2.0",
    "download-file-sync": "^1.0.4",
    "fs-walk": "0.0.2",
    "hammerjs": "^2.0.8",
    "hash-files": "^1.1.1",
    "howler": "^2.1.2",
    "http-server": "^0.11.1",
    "humanize-duration": "^3.21.0",
    "jquery": "^3.4.1",
    "s": "^0.1.1",
    "say": "^0.16.0",
    "sono": "^2.1.6",
    "speak-tts": "^2.0.8",
    "tone": "^13.8.25",
    "unique-random": "^2.1.0"
  },
  "devDependencies": {
    "electron": "^7.1.1",
    "parcel-bundler": "^1.12.4"
  },
  "build": {
    "compression": "maximum",
    "nodeGypRebuild": false,
    "npmRebuild": false,
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "ia32"
          ]
        }
      ]
    }
  }
}
